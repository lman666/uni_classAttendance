page {
  background-color: #fff;
}
.select {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}
.select .uni-list .uni-list-cell {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
}
.select .uni-list .uni-list-cell::after {
  height: 0;
}
.select .uni-list::after {
  height: 0;
}

